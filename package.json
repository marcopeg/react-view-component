{
  "name": "react-view-component",
  "version": "0.0.4",
  "description": "Provides a simple API to space management and layouting for WebApps",
  "main": "./lib/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "clean": "./node_modules/rimraf/bin.js lib coverage dist",
    "lint": "./node_modules/eslint/bin/eslint.js src test/",
    "build:lib": "./node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "build:umd": "./node_modules/webpack/bin/webpack.js src/index.js dist/react-view-component.js --config webpack.config.development.js",
    "build:umd:watch": "./node_modules/webpack/bin/webpack.js src/index.js dist/react-view-component.js --config webpack.config.development.js --watch",
    "build:umd:min": "./node_modules/webpack/bin/webpack.js src/index.js dist/react-view-component.min.js --config webpack.config.production.js",
    "build:lib:watch": "./node_modules/babel-cli/bin/babel.js --watch src --out-dir lib",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive --require ./test/__setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover _mocha",
    "prepublish": "npm run clean && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcopeg/react-view-component.git"
  },
  "files": [
    "lib",
    "src",
    "dist"
  ],
  "keywords": [
    "view",
    "component",
    "layout",
    "library",
    "react",
    "reactjs"
  ],
  "author": "Marco Pegoraro <hello@marcopeg.com> (http://github.com/marcopeg)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcopeg/react-view-component/issues"
  },
  "homepage": "https://github.com/marcopeg/react-view-component#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-nodeca": "^1.0.3",
    "eslint-plugin-react": "^3.15.0",
    "file-loader": "^0.8.5",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11"
  }
}
